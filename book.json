{
    "title": "Gitbook教程",
    "description": "Gitbook电子书教程",
    "author": "Thomas Zhang",
    "language": "zh-hans",
    "root": ".",
    "plugins": [
        "github",
        "ace",
        "theme-comscore",
        "edit-link",
        "tbfed-pagefooter",
        "expandable-chapters",
        "chapter-fold",
        "back-to-top-button",
        "code",
        "pageview-count",
        "popup",
        "splitter",
        "search-plus",
        "-lunr",
        "-search",
        "anchors",
        "mermaid-gb3"
    ],
    "styles": {
        "website": "assets/continuum/cms.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/ThomasZh/gitbook-manual"
        },
        "edit-link": {
            "base": "https://github.com/ThomasZh/gitbook-manual/blob/main/",
            "label": "Edit This Page"
        },
        "tbfed-pagefooter": {
            "copyright": "&copy Cyber-Life.cn Studio",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "pluginsConfig": {
            "expandable-chapters": {}
        }
    }
}
